{
  "name": "@sun-guannan/capcutmaker",
  "version": "0.1.32",
  "description": "CapCut Helper Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --mac --win",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "build": {
    "appId": "com.capcutmaker.app",
    "productName": "CapCutMaker",
    "protocols": {
      "name": "capcutmaker",
      "schemes": [
        "capcutmaker"
      ]
    },
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        {
          "target": "dmg",
          "arch": ["universal"]
        },
        {
          "target": "zip",
          "arch": ["universal"]
        }
      ]
    },
    "linux": {
      "target": []
    },
    "win": {
      "target": "nsis",
      "artifactName": "${productName}-Setup-${version}.exe"
    }
  },
  "keywords": [
    "capcutmaker",
    "electron",
    "desktop"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "electron-log": "^5.4.2",
    "electron-updater": "^6.6.2"
  },
  "devDependencies": {
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12"
  }
}