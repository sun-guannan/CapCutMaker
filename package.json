{
  "name": "@sun-guannan/capcutmaker",
  "version": "0.2.20",
  "description": "CapCut Helper Application",
  "main": "main.js",
  "scripts": {
    "start": "NODE_ENV=development electron .",
    "debug": "electron --inspect=9229 .",
    "build": "webpack && electron-builder --mac --win",
    "build:mac": "webpack && electron-builder --mac",
    "build:win": "webpack && electron-builder --win",
    "dev": "webpack --watch",
    "dev-start": "concurrently \"npm run dev\" \"npm run start\"",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "build": {
    "appId": "com.capcutapi.app",
    "productName": "CapCutAPI",
    "icon": "src/logo.png",
    "protocols": {
      "name": "capcutmaker",
      "schemes": [
        "capcutmaker"
      ]
    },
    "asar": true,
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "package.json",
      "locales/**/*",
      "src/logo.png",
      "src/**/*",
      "public/**/*",
      "util/**/*",
      "template/**/*",
      "template_jianying/**/*"
    ],
    "extraResources": [
      {
        "from": "./resources",
        "to": "resources",
        "filter": [
          "**/*"
        ]
      }
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "asarUnpack": [
        "**/*.node"
      ],
      "icon": "src/logo.icns",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "universal"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "universal"
          ]
        }
      ]
    },
    "linux": {
      "target": []
    },
    "win": {
      "target": "nsis",
      "icon": "src/logo.ico",
      "artifactName": "${productName}-Setup-${version}.exe",
      "extraResources": [
        {
          "from": "locales",
          "to": "locales"
        }
      ]
    }
  },
  "keywords": [
    "capcutapi",
    "electron",
    "desktop"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "adm-zip": "^0.5.16",
    "antd": "^5.27.0",
    "axios": "^1.11.0",
    "electron-log": "^5.4.2",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "fs-extra": "^11.3.1",
    "i18next": "^25.3.6",
    "i18next-fs-backend": "^2.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.8.1",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
