{
  "name": "capcutmaker",
  "version": "0.1.2",
  "description": "CapCut Helper Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --mac --win",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win",
    "test": "echo \"No tests specified\" && exit 0",
    "publish": "electron-builder --publish always"
  },
  "build": {
    "appId": "com.capcutmaker.app",
    "productName": "CapCutMaker",
    "protocols": {
      "name": "capcutmaker",
      "schemes": [
        "capcutmaker"
      ]
    },
    "mac": {
      "category": "public.app-category.utilities",
      "target": {
        "target": "dmg",
        "arch": [
          "universal"
        ]
      },
      "publish": [
        "github"
      ]
    },
    "win": {
      "target": "nsis",
      "publish": [
        "github"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "sunguannan",
        "repo": "CapCutMaker"
      }
    ]
  },
  "keywords": [
    "capcutmaker",
    "electron",
    "desktop"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "electron-log": "^5.4.2",
    "electron-updater": "^6.6.2"
  }
}
